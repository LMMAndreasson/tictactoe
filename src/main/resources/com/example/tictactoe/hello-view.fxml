<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.tictactoe.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" />
    <Button onAction="#restart" text="Restart" />
   <GridPane gridLinesVisible="false" prefHeight="300.0" prefWidth="300.0">
     <columnConstraints>
       <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="60.0" />
       <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="60.0" />
       <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="10.0" />

     </columnConstraints>
     <rowConstraints>
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
     </rowConstraints>
      <children>
         <Button fx:id="button0" mnemonicParsing="false" onAction="#buttonAction" prefHeight="60.0" prefWidth="60.0" text="" />
         <Button fx:id="button1" mnemonicParsing="false" onAction="#buttonAction" prefHeight="60.0" prefWidth="60.0" text="" GridPane.columnIndex="1" />
         <Button fx:id="button2" mnemonicParsing="false" onAction="#buttonAction" prefHeight="60.0" prefWidth="60.0" text="" GridPane.columnIndex="2" />
         <Button fx:id="button3" mnemonicParsing="false" onAction="#buttonAction" prefHeight="60.0" prefWidth="60.0" text="" GridPane.rowIndex="1" />
         <Button fx:id="button4" mnemonicParsing="false" onAction="#buttonAction" prefHeight="60.0" prefWidth="60.0" text="" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         <Button fx:id="button5" mnemonicParsing="false" onAction="#buttonAction" prefHeight="60.0" prefWidth="60.0" text="" GridPane.columnIndex="2" GridPane.rowIndex="1" />
         <Button fx:id="button6" mnemonicParsing="false" onAction="#buttonAction" prefHeight="60.0" prefWidth="60.0" text="" GridPane.rowIndex="2" />
         <Button fx:id="button7" mnemonicParsing="false" onAction="#buttonAction" prefHeight="60.0" prefWidth="60.0" text="" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         <Button fx:id="button8" mnemonicParsing="false" onAction="#buttonAction" prefHeight="60.0" prefWidth="60.0" text="" GridPane.columnIndex="2" GridPane.rowIndex="2" />
      </children>
   </GridPane>
   <GridPane>
     <columnConstraints>
       <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="60.0" />
       <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="60.0" />
         <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="10.0" />
     </columnConstraints>
     <rowConstraints>
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
     </rowConstraints>
      <children>
         <Label fx:id="welcomeText1" text="Player 1" />
         <Label fx:id="welcomeText111" text="Player 2" GridPane.columnIndex="2" />
         <Label fx:id="welcomeText11" text="Draws" GridPane.columnIndex="1" />
          <Label fx:id="p1Score" text="0" GridPane.rowIndex="1" />
          <Label fx:id="drawScore" prefHeight="17.0" prefWidth="0.0" text="0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
          <Label fx:id="p2Score" text="0" GridPane.columnIndex="2" GridPane.rowIndex="1" />
      </children>
   </GridPane>

</VBox>
